<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginValidate</servlet-name>
  	<display-name>LoginValidate</display-name>
  	<description></description>
  	<servlet-class>com.dxc.servlet.LoginValidate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Update</servlet-name>
  	<display-name>Update</display-name>
  	<description></description>
  	<servlet-class>com.dxc.servlet.Update</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Delete</servlet-name>
  	<display-name>Delete</display-name>
  	<description></description>
  	<servlet-class>com.dxc.servlet.Delete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveStudent</servlet-name>
  	<display-name>SaveStudent</display-name>
  	<description></description>
  	<servlet-class>com.dxc.servlet.SaveStudent</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginValidate</servlet-name>
  	<url-pattern>/loginvalidate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Update</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveStudent</servlet-name>
  	<url-pattern>/save</url-pattern>
  </servlet-mapping>
</web-app>
